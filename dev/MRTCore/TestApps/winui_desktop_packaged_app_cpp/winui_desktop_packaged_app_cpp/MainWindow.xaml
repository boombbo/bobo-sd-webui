<Window
    x:Class="winui_desktop_packaged_app_cpp.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:winui_desktop_packaged_app_cpp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button x:Name="defaultWinrtApiButton" Click="defaultWinrtApi_Click" Width="400">Default Language (uses WinRT APIs)</Button>
        <Button x:Name="overrideWinrtApiButton" Click="overrideWinrtApi_Click" Width="400" Margin="0,20,0,0" >Override Language (uses WinRT APIs)</Button>
        <Button x:Name="defaultCApiButton" Click="defaultCApi_Click" Width="400" Margin="0,20,0,0" >Default Language (uses C APIs)</Button>
        <Button x:Name="overrideCApiButton" Click="overrideCApi_Click" Width="400" Margin="0,20,0,0" >Override Language (uses C APIs)</Button>
        <Button x:Name="fallbackWinrtApiButton" Click="fallbackWinrt_Click" Width="400" Margin="0,20,0,0" >Resource fallback (uses WinRT APIs)</Button>
        <TextBox x:Name="output" Width="400" Margin="0,20,0,0" Text="" />
    </StackPanel>
</Window>
